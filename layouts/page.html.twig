            /* Notes */
            main .note {
                position: relative;
                padding: 1rem;
                margin-bottom: 1rem;
                border-radius: 0.25rem;
            }
            main .note > p {
                margin: 0;
            }
            main .note-info {
                background-color: rgb(var(--rgb-blue), 50%);
                border: 1px solid rgb(var(--rgb-blue));
                border-left-width: 0.5rem;
            }
            main .note-tip {
                background-color: rgb(var(--rgb-green), 50%);
                border: 1px solid rgb(var(--rgb-green));
                border-left-width: 0.5rem;
            }
            main .note-important {
                border-left-width: 0.5rem;
            }
            main .note-warning {
                border-left-width: 0.5rem;
            }
            main .note-caution {
                border-left-width: 0.5rem;
            }

            main pre {
                background-color: #002000;
                padding: 0.5rem 1rem;
                border-radius: 0.375rem;
            }

            main > img {
                border: 2px solid var(--theme-fg);
            }

            main > img.rounded {
                border-radius: 50%;
            }

            main > img.centered {
                display: block;
                margin-left: auto;
                margin-right: auto;
            }

            main > img.rounded-centered {
                border-radius: 50%;
                display: block;
                margin-left: auto;
