                --theme-green-dark: #2e3220;
                --theme-green: #608342;
                --theme-green-light: #c6d8c6;
                --theme-orange: #f2ba57;
                --theme-orange-light: #f7d18e;

            }

            body {
                /*font-family: "Courier New", Courier, "Lucida Sans Typewriter", "Lucida Typewriter", monospace;*/
                font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
                min-height: 100vh;
                margin: 1rem;
                background-color: var(--theme-bg);
                color: var(--theme-fg);
                line-height: 1.75rem;
            }

            .centered {
                margin: 0 auto;
                max-width: 800px;
            }

            hr {
                border-width: 0; /* overwrite default width */
                border-top: 1px solid var(--theme-orange);
                height: 0;
                width: 100%;
            }

            .title {
                font-weight: bold;
                font-size: 1.25rem;
            }

            h1 {
                color: var(--theme-orange);
                margin-top: 4rem;
            }

            h2 { margin-top: 4rem; }

            a {
                color: var(--theme-orange);
                text-decoration: none;
            }
            a:hover, a:visited {
                color: var(--theme-orange);
                text-decoration: underline dashed;
                text-underline-offset: 4px;
            }


